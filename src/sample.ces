/* Factorial en CEs */

entero a;

entero fa (entero x, entero y) {
    retorno 1;
}

sin_tipo main() {
    entero x;
    entero fact;
    fact = fa();
    x = entrada(); /* leer un entero */
    fact = 1;
    si (x < 0) retorno;
    fact = fact * x;
    x = x - 1;
    mientras (x = 0) {
        fact = fact * x;
        x = x - 1;
    }
    salida(fact);
}